jessie,             deb http://ftp.de.debian.org/debian/ jessie main  #  deb http://ftp.de.debian.org/debian-security/ jessie/updates main           , ,
wheezy-backports,   deb http://ftp.de.debian.org/debian/ wheezy-backports main , ,
wheezy,             deb http://ftp.de.debian.org/debian/ wheezy main  #  deb http://ftp.de.debian.org/debian-security/ wheezy/updates main           , ,
